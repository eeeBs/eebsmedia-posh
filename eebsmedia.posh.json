{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "foreground": "#ffffff",
          "background": "#cc3802",
          "template": "{{ if .Env.PNPPSHOST }} \uf8c5 {{ .Env.PNPPSHOST }} {{ end }}"
        },
        {
          "type": "text",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#047e84",
          "powerline_symbol": "\ue0b0",
          "template": "{{ if .Env.PNPPSSITE }} \uf672 {{ .Env.PNPPSSITE }}{{ end }}"
        },
        {
          "type": "text",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#047e84",
          "template": "{{ if .Env.PNPPSSITE }}\u00A0{{ end }}"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#513875",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "üçï {{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#ffcb56",
          "foreground": "#333333",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        
        {
          "background": "#ff6566",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "accordion",
          "template": " \u279c ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}) ",
          "type": "git"
        },
        {
          "type": "php",
          "style": "accordion",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#35afe7",
          "template": " \ue73d {{ .Full }} "
        },
        {
          "type": "node",
          "background": "#06969a",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "accordion",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
        },
        {
          "type": "time",
          "background": "#2b6ac2",
          "foreground": "#FFF",
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "template": " \u2665 <b>{{ .CurrentDate | date .Format }}</b> ",
          "trailing_diamond": "\ue0b0"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}